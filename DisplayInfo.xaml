<Page x:Class="Individual_project_initial.DisplayInfo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Individual_project_initial"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1280"
            Title="DisplayInfo" Height="720" Width="1280">
    <Viewbox Stretch="Uniform">
        <Grid Height="720" Width="1280" Background="White">
            <Separator HorizontalAlignment="Left" Height="15" Margin="120,50,0,0" VerticalAlignment="Top" Width="355"/>
            <TextBlock Text="Results" FontSize="30" HorizontalAlignment="Left" Margin="120,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="39" Width="170"/>
            <ScrollViewer Margin="120,70,0,10" HorizontalAlignment="Left" Width="733">
                <StackPanel x:Name="ResultsStackPanel" Width="719" Height="auto">
                </StackPanel>
            </ScrollViewer>
        </Grid>
    </Viewbox>
</Page>
