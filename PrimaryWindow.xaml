<Window x:Class="Individual_project_initial.PrimaryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Individual_project_initial"
        mc:Ignorable="d"
        Title="PrimaryWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="Frame" NavigationUIVisibility="Hidden"/>
        <DockPanel>
            <ListBox DockPanel.Dock="Left" Width="auto" Background="LightGray" HorizontalAlignment="Left">
                <ListBox.ItemContainerStyle>
                    <Style TargetType="ListBoxItem">
                        <Setter Property="VerticalAlignment" Value="Stretch" />
                    </Style>
                </ListBox.ItemContainerStyle>
                <Button Content="Home" Click="GoToHome_Click" />
                <Button Content="Accounts" Click="GoToAccounts_Click" />
                <Button Content="Targets" Click="GoToTargets_Click" />
                <Button Content="Reminders" Click="GoToReminders_Click" />
                <Button Content="Interest" Click="GoToInterest_Click" />
                <Button Content="Markets" Click="GoToMarkets_Click" />
                <Button Content="Settings" Click="GoToSettings_Click" VerticalAlignment="Bottom" />
                <Button Content="Log out" Click="Logout_Click" VerticalAlignment="Bottom" />
            </ListBox>
        </DockPanel>
    </Grid>
</Window>
