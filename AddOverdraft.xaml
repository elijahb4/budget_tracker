<Page x:Class="Individual_project_initial.AddOverdraft"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Individual_project_initial"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AddOverdraft">
    <Grid>
        <TextBox Name="overdraftLimitTextBox" HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" Text="Overdraft Limit" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="overdraftInterestRateTextBox" HorizontalAlignment="Center" Margin="0,33,0,0" TextWrapping="Wrap" Text="Overdraft Interest Rate" VerticalAlignment="Top" Width="120"/>
        <Button Content="Add Account" Click="SubmitButton_Click" HorizontalAlignment="Center" Margin="0,242,0,0" VerticalAlignment="Top"/>
    </Grid>
</Page>
