<Window x:Class="Individual_project_initial.Transfer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Individual_project_initial"
        mc:Ignorable="d"
        Title="Transfer" Height="720" Width="1280">
    <Viewbox Stretch="Uniform">
        <Grid Height="720" Width="1280">
            <TextBlock HorizontalAlignment="Left" Height="41" Margin="120,10,0,0" TextWrapping="Wrap" Text="Transfer" FontSize="30" VerticalAlignment="Top" Width="314"/>
            <Separator HorizontalAlignment="Left" Height="15" Margin="120,50,0,0" VerticalAlignment="Top" Width="355"/>
            <Label Content="Account From" HorizontalAlignment="Left" Margin="120,65,0,0" VerticalAlignment="Top" Height="32" Width="120"/>
            <ComboBox Name="AccountFromComboBox" HorizontalAlignment="Left" Margin="120,97,0,0" VerticalAlignment="Top" Width="120"/>
            <Label Content="Account To" HorizontalAlignment="Left" Margin="120,127,0,0" VerticalAlignment="Top" Height="30" Width="120"/>
            <ComboBox Name="AccountToComboBox" HorizontalAlignment="Left" Margin="120,162,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="AccountToComboBox_SelectionChanged"/>
            <Label FontSize="14" Content="Sum" HorizontalAlignment="Left" Margin="120,192,0,0" VerticalAlignment="Top" Height="30" Width="120"/>
            <TextBox Name="SumTextBox" HorizontalAlignment="Left" Margin="120,222,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="25"/>
            <Button Content="Transfer" Click="TransferButton_Click" HorizontalAlignment="Left" Margin="120,275,0,0" VerticalAlignment="Top" Width="100"/>
        </Grid>
    </Viewbox>
</Window>
